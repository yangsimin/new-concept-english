    {
      "voaid": "1051",
      "EndTiming": "17.2",
      "Paraid": "1",
      "IdIndex": "5",
      "Timing": "15.2",
      "Sentence_cn": "气候非常宜人。",
      "Sentence": "It's very pleasant."
    },
    {
      "voaid": "1051",
      "EndTiming": "20.5",
      "Paraid": "1",
      "IdIndex": "6",
